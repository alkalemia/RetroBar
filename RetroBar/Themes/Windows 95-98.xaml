<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="ButtonHighlight" Color="#ffffff" />
    <SolidColorBrush x:Key="ButtonInnerHighlight" Color="#ffffff" />
    <SolidColorBrush x:Key="ButtonLight" Color="#ffffff" />
    <SolidColorBrush x:Key="ButtonFace" Color="#F7E1FB" />
    <SolidColorBrush x:Key="ButtonShadow" Color="#815CDD" />
    <SolidColorBrush x:Key="ButtonDarkShadow" Color="##815CDD" />
    <SolidColorBrush x:Key="ButtonFlashingFace" Color="#4D23CF" />

    <SolidColorBrush x:Key="ButtonForeground" Color="#4D21CB" />
    <SolidColorBrush x:Key="ButtonFlashingForeground" Color="#4D21CB" />
    <SolidColorBrush x:Key="ClockForeground" Color="#4D21CB" />
    
    <SolidColorBrush x:Key="ToolTipBackground" Color="#CCE4FC" />
    <SolidColorBrush x:Key="ToolTipForeground" Color="#4D21CB" />

    <ToolTip x:Key="StartButtonTip" Content="{DynamicResource start_button_tip_98}" />

    <FontFamily x:Key="GlobalFontFamily">Zpix</FontFamily>
</ResourceDictionary>
